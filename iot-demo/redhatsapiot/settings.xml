  <profile>
      <id>repo-eclipse</id>
      <repositories>
        <repository>
          <id>eclipse.org</id>
          <name>eclipse paho repo</name>
          <url>https://repo.eclipse.org/content/repositories/paho-releases</url>
        </repository>
      </repositories>
      <pluginRepositories>
        <pluginRepository>
          <id>eclipse.org</id>
          <name>eclipse paho repo</name>
          <url>https://repo.eclipse.org/content/repositories/paho-releases</url>
        </pluginRepository>
      </pluginRepositories>
    </profile>
  </profiles>
  <activeProfiles>
    <activeProfile>jboss-ga-repository</activeProfile>
  
  <activeProfile>repo-eclipse</activeProfile>
  </activeProfiles>
</settings>
